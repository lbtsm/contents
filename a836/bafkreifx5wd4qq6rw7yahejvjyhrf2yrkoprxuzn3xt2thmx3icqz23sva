let eventCenter = new Entity();
eventCenter.addComponent(new Transform({
  position: new Vector3(-10 * 16,0,-9 * 16),
  scale: new Vector3().set(0.9, 1, 1)
}))

// eventCenter.addComponent(new GLTFShape("models/Event_Center.glb"))
eventCenter.addComponent(new GLTFShape("models/DAO_Center.glb"))
engine.addEntity(eventCenter)

