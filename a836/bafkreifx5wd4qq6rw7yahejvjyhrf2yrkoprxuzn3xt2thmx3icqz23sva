let centralParkCenter = new Entity();


centralParkCenter.addComponent(new Transform({
  position: new Vector3(8,0,8)
}))

centralParkCenter.addComponent(new GLTFShape("models/Central_Park_Center.glb"))

engine.addEntity(centralParkCenter)

